<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="findarrow.rian.andre.com.br.findarrow.MainActivity">

    <RelativeLayout
        android:layout_width="667dp"
        android:layout_height="323dp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_height="match_parent"
        tools:layout_width="match_parent">

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="@dimen/lagura_botao"
            android:layout_height="@dimen/altura_botao"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="18dp"
            android:layout_marginStart="18dp"
            android:layout_marginTop="18dp"
            app:srcCompat="@mipmap/ic_launcher" />

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="@dimen/lagura_botao"
            android:layout_height="@dimen/altura_botao"
            app:srcCompat="@mipmap/ic_launcher"
            android:layout_alignParentBottom="true"
            android:layout_alignLeft="@+id/imageView9"
            android:layout_alignStart="@+id/imageView9"
            android:layout_marginBottom="18dp" />

        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="@dimen/lagura_botao"
            android:layout_height="@dimen/altura_botao"
            app:srcCompat="@mipmap/ic_launcher"
            android:layout_centerVertical="true"
            android:layout_alignLeft="@+id/imageView"
            android:layout_alignStart="@+id/imageView"
            android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" />

        <ImageView
            android:id="@+id/imageView10"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/altura_botao"
            android:layout_alignLeft="@+id/imageView6"
            android:layout_alignStart="@+id/imageView6"
            android:layout_alignTop="@+id/imageView5"
            android:layout_marginLeft="12dp"
            android:layout_marginStart="12dp"
            android:background="@mipmap/ic_launcher"
            tools:layout_height="@dimen/altura_botao"
            tools:layout_width="@dimen/lagura_botao" />

        <ImageView
            android:id="@+id/imageView11"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/altura_botao"
            android:layout_alignTop="@+id/imageView2"
            android:layout_marginLeft="25dp"
            android:layout_marginStart="25dp"
            android:layout_toEndOf="@+id/imageView2"
            android:layout_toRightOf="@+id/imageView2"
            app:srcCompat="@mipmap/ic_launcher"
            tools:layout_centerHorizontal="true"
            tools:layout_width="@dimen/lagura_botao" />

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/altura_botao"
            android:layout_alignTop="@+id/imageView"
            android:layout_marginLeft="26dp"
            android:layout_marginStart="26dp"
            android:layout_toEndOf="@+id/imageView"
            android:layout_toRightOf="@+id/imageView"
            android:background="@mipmap/ic_launcher"
            tools:layout_height="@dimen/altura_botao"
            tools:layout_width="@dimen/lagura_botao" />

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/altura_botao"
            app:srcCompat="@mipmap/ic_launcher"
            tools:layout_centerHorizontal="true"
            tools:layout_width="@dimen/lagura_botao"
            android:layout_alignBottom="@+id/imageView10"
            android:layout_alignLeft="@+id/imageView11"
            android:layout_alignStart="@+id/imageView11"
            android:layout_marginBottom="7dp" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/altura_botao"
            android:background="@mipmap/ic_launcher"
            tools:layout_height="@dimen/altura_botao"
            tools:layout_width="@dimen/lagura_botao"
            android:layout_alignTop="@+id/imageView9"
            android:layout_alignLeft="@+id/imageView6"
            android:layout_alignStart="@+id/imageView6" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/altura_botao"
            android:layout_alignLeft="@+id/imageView7"
            android:layout_alignStart="@+id/imageView7"
            android:layout_alignTop="@+id/imageView6"
            app:srcCompat="@mipmap/ic_launcher"
            tools:layout_centerHorizontal="true"
            tools:layout_width="@dimen/lagura_botao" />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="@dimen/lagura_botao"
            android:layout_height="@dimen/altura_botao"
            android:layout_marginEnd="21dp"
            android:layout_marginRight="21dp"
            app:srcCompat="@mipmap/ic_launcher"
            tools:layout_alignTop="@+id/imageView3"
            android:layout_alignTop="@+id/imageView3"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />
    </RelativeLayout>

</android.support.constraint.ConstraintLayout>
